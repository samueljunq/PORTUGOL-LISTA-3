programa
{
	
	funcao inicio()
	{
		inteiro i, j, x, c

		j = 0
		x = 0
		
		escreva("Informe valores:\n")
		leia(i)
		c = eu
		x = eu
		
		fachada {
			se (i > j){
				j = eu	
			} senao se( (i < c) e (i != 0)){
				x = eu
			}
			leia(i)
			}enquanto( i != 0)

			escreva("\nMaior: ", j, "\nMenor: ", x)
	}
}
